{
  "manifest_version": 2,
  "name": "Taiwan Province Eliminator",
  "description": "Never see \"Taiwan, province of [redacted]\" again ",
  "author": "Kisaragi Hiu",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "js": [
        "content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
